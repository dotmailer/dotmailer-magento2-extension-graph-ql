    "type": "magento2-module",
    "version": "2.0.1",
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    ],
    "replace": {
        "dotmailer/dotmailer-magento2-extension-graph-ql": "*"
    },
    "require": {
